{
  "name": "Gotime",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href / --deploy-url / --host gotime.shambhalamountain.org --ssl true --ssl-key ~/.private/smc.key --ssl-cert ~/.private/smc.crt --port 8443",
    "build-prod": "ng build --base-href='/' --environment=prod --deploy-url='/' --output-path=public",
    "build-dev": "ng build --base-href='/' --environment=dev --deploy-url='/' --output-path=public-dev",
    "deploy-prod": "firebase deploy --only hosting",
    "deploy-dev": "firebase deploy -p public-dev -P smc-internal-dev --only hosting",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@types/auth0-js": "^8.11.0",
    "@types/firebase": "^2.4.32",
    "angularfire2": "^5.0.0-rc.4",
    "auth0-js": "^8.12.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.1",
    "firebase-admin": "latest",
    "gulp": "^3.9.1",
    "karma-firefox-launcher": "^1.0.1",
    "rxjs": "^5.5.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.2",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.92",
    "codelyzer": "^3.2.2",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.2",
    "ts-node": "^3.2.2",
    "tslint": "^5.7.0",
    "typescript": ">=2.4.2 <2.6"
  },
